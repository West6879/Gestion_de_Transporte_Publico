<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="601.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="visual.ListRutaController">
    <children>
        <TableView fx:id="tablaRutas" layoutX="200.0" layoutY="55.0" prefHeight="294.0" prefWidth="600.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
            <columns>
                <TableColumn fx:id="colOrigen" prefWidth="120.0" resizable="false" sortable="false" text="Estación Origen" />
                <TableColumn fx:id="colDestino" prefWidth="120.0" resizable="false" sortable="false" text="Estación Destino" />
                <TableColumn fx:id="colDistancia" prefWidth="80.0" resizable="false" sortable="false" text="Distancia" />
                <TableColumn fx:id="colTiempo" prefWidth="80.0" resizable="false" sortable="false" text="Tiempo" />
                <TableColumn fx:id="colCosto" prefWidth="80.0" resizable="false" sortable="false" text="Costo" />
                <TableColumn fx:id="colPonderacion" prefWidth="100.0" resizable="false" sortable="false" text="Ponderación" />
            </columns>
        </TableView>
        <ButtonBar fx:id="btnBar" layoutX="200.0" layoutY="360.0" prefHeight="40.0" prefWidth="200.0" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
            <buttons>
                <Button fx:id="btnModificar" disable="true" mnemonicParsing="false" onAction="#btnModificarClicked" text="Modificar" />
                <Button fx:id="btnEliminar" disable="true" mnemonicParsing="false" onAction="#btnEliminarClicked" text="Eliminar" />
                <Button fx:id="btnCancelar" cancelButton="true" mnemonicParsing="false" onAction="#btnCancelarClicked" text="Cancelar" />
            </buttons>
        </ButtonBar>
        <Label alignment="CENTER" layoutX="229.0" layoutY="14.0" prefHeight="35.0" prefWidth="142.0" text="Rutas">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
    </children>
</AnchorPane>